<Window x:Class="Result_of_run.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Result_of_run"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <DataGrid AutoGenerateColumns="False" x:Name="ResultTable">
            <DataGrid.Columns>
                <DataGridTextColumn Binding = "{Binding Marathon}" Header="Marathon" Width="auto"/>
                <DataGridTextColumn Binding = "{Binding Type}" Header="Type" Width="auto"/>
                <DataGridTextColumn Binding = "{Binding Time}" Header="Time" Width="auto"/>
                <DataGridTextColumn Header="Общее место"/>
                <DataGridTextColumn Header="Место по категории"/>
            </DataGrid.Columns>
        </DataGrid>
    </Grid>
</Window>
